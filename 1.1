from flask import Flask, request
app = Flask(__name__)
seed = 0
@app.route('/', methods=['GET'])
def get_seed():
    return str(seed)
@app.route('/', methods=['POST'])
def update_seed():
    global seed
    seed = request.json['num']
    return 'Success!'
if __name__ == '__main__':
    app.run(host='0.0.0.0', port=5000)
